{
  "name": "bitmagic.vsc",
  "version": "1.0.0",
  "description": "BitMagic VSC Plugin",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.61.0"
  },
  "author": "Yazwho?",
  "license": "ISC",
  "contributes": {
    "customEditors": [
      {
        "viewType": "bitmagic.csasm",
        "displayName": "BitMagic CS Razor ASM",
        "selector": [
          {
            "filenamePattern": "*.csasm"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "typescript": "^4.4.4"
  }
}
